{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "add-module-exports",
    "babel-root-import",
    "react-require",
    ["provide-modules", {
      "debug": "debug"
    }],
    "transform-decorators-legacy",
    ["module-alias", [
      { "src": "./src/app", "expose": "app" },
      { "src": "./src/assets", "expose": "assets" },
      { "src": "./src/config", "expose": "config" },
      { "src": "./src/server", "expose": "server" },
      { "src": "./src/styles", "expose": "styles" }
    ]]
  ]
}
